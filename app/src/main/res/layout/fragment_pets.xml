<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    tools:context=".ui.devices.DevicesFragment"
    android:id="@+id/pets"
    android:orientation="vertical">



        <dev.oneuiproject.oneui.widget.Separator
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="@string/cat"/>
    <dev.oneuiproject.oneui.widget.RoundLinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginTop="10dp"
        android:background="@color/oui_background_color"
        android:orientation="vertical">
    <com.bohdan157.app.ui.widget.CardView
        android:id="@+id/acaaa"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="vertical"
        app:TitleText="@string/TName"
        app:SummaryText="@string/Cname">
    </com.bohdan157.app.ui.widget.CardView>
    <com.bohdan157.app.ui.widget.CardView
        android:id="@+id/acaaaa"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="vertical"
        app:TitleText="@string/breed"
        app:SummaryText="@string/Cbreed">
    </com.bohdan157.app.ui.widget.CardView>
    <com.bohdan157.app.ui.widget.CardView
        android:id="@+id/acaas"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="vertical"
        app:TitleText="@string/bday"
        app:SummaryText="12.09.2024">
    </com.bohdan157.app.ui.widget.CardView>
    </dev.oneuiproject.oneui.widget.RoundLinearLayout>

        <dev.oneuiproject.oneui.widget.Separator
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="@string/hamster" />

    <dev.oneuiproject.oneui.widget.RoundLinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginTop="10dp"
        android:background="@color/oui_background_color"
        android:orientation="vertical">
    <com.bohdan157.app.ui.widget.CardView
        android:id="@+id/acaad"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="vertical"
        app:TitleText="@string/TName"
        app:SummaryText="@string/Hname">
    </com.bohdan157.app.ui.widget.CardView>
    <com.bohdan157.app.ui.widget.CardView
        android:id="@+id/acaaq"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="vertical"
        app:TitleText="@string/breed"
        app:SummaryText="@string/Djunharian">
    </com.bohdan157.app.ui.widget.CardView>
    <com.bohdan157.app.ui.widget.CardView
        android:id="@+id/acaaqa"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="vertical"
        app:TitleText="@string/bday"
        app:SummaryText="23.10.2023">
    </com.bohdan157.app.ui.widget.CardView>
    </dev.oneuiproject.oneui.widget.RoundLinearLayout>
</LinearLayout>